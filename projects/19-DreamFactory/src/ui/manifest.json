{
  "manifest_version": 3,
  "name": "Dream Factory Chrome Extension",
  "version": "1.0",
  "description": "This is one open source project NFT Chrome extension",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon32.png",
      "128": "images/icon192.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
